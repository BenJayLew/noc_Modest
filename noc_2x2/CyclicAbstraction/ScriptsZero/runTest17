#!/bin/bash

for loc0 in 'true false'; do
    for ns0 in 'true false'; do
        for loc1 in 'true false'; do
            for ns1 in 'true false'; do
                for loc2 in 'true false'; do
                    for ns2 in 'true false'; do
                        for loc3 in 'true false'; do
                            for ns3 in 'true false'; do
                                mcsta ../CyclicalTest.modest -E "DUR = 10, Mvar = 3, Nvar = 7, Startvar = 0, NSPRI0 = $ns0, LOCPRI0 = $loc0, NSPRI1 = $ns1, \
                                LOCPRI1 = $loc1, NSPRI2 = $ns2, LOCPRI2 = $loc2, NSPRI3 = $ns3, LOCPRI3 = $loc3" --unsafe -O Results/17$loc0$ns0$loc1$ns1$loc2$ns2$loc3$ns3.txt Minimal --props "p1024, p1025, p1026, p1027, p1028, p1029, p1030, p1031, p1032, p1033, p1034, p1035, p1036, p1037, p1038, p1039, p1040, p1041, p1042, p1043, p1044, p1045, p1046, p1047, p1048, p1049, p1050, p1051, p1052, p1053, p1054, p1055, p1056, p1057, p1058, p1059, p1060, p1061, p1062, p1063, p1064, p1065, p1066, p1067, p1068, p1069, p1070, p1071, p1072, p1073, p1074, p1075, p1076, p1077, p1078, p1079, p1080, p1081, p1082, p1083, p1084, p1085, p1086"
                            done
                        done
                    done
                done
            done
        done
    done
done